from matplotlib import pyplot as plt
import numpy as np
import scipy.constants as spc
from scipy.optimize import curve_fit
from scipy.constants import u

' constants'
c     = spc.c
eps_0 = spc.epsilon_0
Z_0   = spc.physical_constants['characteristic impedance of vacuum'][0]
pi    = np.pi
k_B   = spc.k
h     = spc.h
hbar  = h/(2*pi)

' parameters '
lamb    = 759e-9    
k       = 2*pi/lamb     
m_Yb174 = 174*u

alpha_oscar   = -1.8191526973423665e-39
alpha_oscar_1 = alpha_oscar/(2*eps_0*c)
